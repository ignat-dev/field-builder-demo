{
  "name": "field-builder-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build --turbopack",
    "build:themes": "node src/scripts/buildThemes.js",
    "dev": "next dev --turbopack",
    "deploy": "cross-env NODE_ENV=production npm run build && firebase deploy --only hosting",
    "lint": "eslint",
